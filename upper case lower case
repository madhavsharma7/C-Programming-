#include<stdio.h>
int main()
{
	char ch;
	printdf("Enter a character\n");
	scanf("%c",&ch);
    if(ch >=64 && ch<=91)
	{
		printf("%c is an uppercase alphabet\n", ch);
	}
	else if(ch>=96 && ch<=123)
	{
		printf("%c is a an lowercase alphabet\n",ch);
	}		
	ptintf("%c is a spacial character\n",ch);
	
}